{
    "version": 2,
    "outputDirectory": ".",
    "headers": [
        {
            "source": "/(.*)\\.mp3",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "audio/mpeg"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)\\.ogg",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "audio/ogg"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)\\.wav",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "audio/wav"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}